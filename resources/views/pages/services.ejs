<!-- BREADCRUMB -->
<%- include("../components/breadcrumb-area", { title: "Servicios" }) %>

<!-- start: Service Area -->
<section class="service-section">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="section-header text-center">
                    <h2 class="section-title wow fadeInUp" data-wow-delay=".3s">Desarrollo de Alto Rendimiento</h2>
                    <p class="wow fadeInUp text-uppercase" data-wow-delay=".4s">Especialidades</p>
                </div>
            </div>
        </div>
        <div class="row">
            <% data.services.forEach(service => { %>
                <div class="col-lg-6">
                    <div class="service-item wow fadeInLeft" data-wow-delay=".3s">
                        <div class="service-icon">
                            <i class="<%= service.icon %>"></i>
                        </div>
                        <div class="service-content">
                            <h5 class="title">
                                <%= service.title %>
                            </h5>
                            <div class="desc">
                                <p>
                                    <%= service.description %>
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
            <% }); %>
        </div>
    </div>
</section>
<!-- end: Service Area -->